<div class='ui six wide column'>
  <h1>All Students</h1>
  <% if logged_in? %>
    <h1>Welcome, <%= current_user.username %></h1>
  <% end %>
  <div class= 'ui huge middle aligned selection divided list'>
    <% @students.each do |student| %>
      <div class="item">
        <%= link_to student.name, student_path(student) %>
      </div>
    <% end %>
  </div>
</div>
